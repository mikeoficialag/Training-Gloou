<mvc:View controllerName="ns.project1.controller.Lista"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:sf="sap.ui.comp.smartform"
    >
    <Page id="page" title="{i18n>title}">
      <content> 

 <Panel id="pan1">
    
<FlexBox id="fxl" justifyContent="End" >
 <VBox id="vb">

  <l:VerticalLayout class="sapUiContentPadding" id="vl1">
    <Button id="btn1" icon="sap-icon://log" text="Log-out" press="onPressLogOut"/>
    <Button id="ope" icon="sap-icon://accidental-leave"
    
			text="{i18n>btnListDialog}"
			press=".openDialog"
			ariaHasPopup="Dialog" />
    </l:VerticalLayout>
    </VBox>
  </FlexBox>
  </Panel>

  <Panel id="panel2">
 <List id="lista1" headerText="{i18n>titleList}" items="{NWProducts>/products}">
  <StandardListItem type="Active" busy="{NWProducts>/isBusy}" id="sli1" title="{NWProducts>ProductName}"/>
  </List>
  </Panel>
       </content>
    </Page>
</mvc:View>
